                <em:maxVersion>@ADDON_TARGET_APP_MAXVER@</em:maxVersion>
                <em:updateLink>https://addons.palemoon.org/media/addons/@ADDON_APMO_TYPE@/@ADDON_APMO_ID@/@ADDON_XPI_NAME@-@ADDON_VERSION@.xpi</em:updateLink>
                <em:updateHash>sha256:@ADDON_HASH_SHA256@</em:updateHash>
              </RDF:Description>
            </em:targetApplication>
          </RDF:Description>
        </RDF:li>
      </RDF:Seq>
    </em:updates>
  </RDF:Description>
</RDF:RDF>