# For targeted overlays use 'overlay   chrome://uri application=@ADDON_TARGET_APP_ID@'

oam

content oam content/
skin oam classic/1.0 skin/winstripe/ os=WINNT
skin oam classic/1.0 skin/pinstripe/ os=Darwin
skin oam classic/1.0 skin/gnomestripe/ os=Linux

overlay chrome://browser/content/browser.xul                   chrome://oam/content/appOverlay.xul
overlay chrome://browser/content/macBrowserOverlay.xul         chrome://oam/content/appOverlay.xul
overlay chrome://browser/content/webPanels.xul                 chrome://oam/content/appOverlay.xul
overlay about:addons                                           chrome://oam/content/aboutAddonsOverlay.xul

style chrome://mozapps/content/extensions/extensions.xul       chrome://oam/content/layout.css
style about:addons                                             chrome://oam/content/layout.css
overlay chrome://mozapps/content/extensions/extensions.xul     chrome://oam/content/extensionsOverlay.xul
overlay about:addons                                           chrome://oam/content/extensionsOverlay.xul
override chrome://mozapps/skin/extensions/extensions.css       chrome://oam/skin/extensions.css

#Stylish
override chrome://stylish/content/manage-addons-fx4.xul        chrome://oam/content/stylishOverlay.xul

#Greasemonkey
overlay  chrome://mozapps/content/extensions/extensions.xul    chrome://greasemonkey/content/addons4-overlay.xul
override chrome://greasemonkey/content/addons4-overlay.xul     chrome://oam/content/gmOverlay.xul

#Locales
locale oam en-US locale/en-US/